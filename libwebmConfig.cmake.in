#-----------------------------------------------------------------------------
#
# libwebmConfig.cmake - libwebm CMake configuration file for external projects.
#

# The libwebm include file directories.
SET(LIBWEBM_INCLUDE_DIRS "@LIBWEBM_INCLUDE_DIRS_CONFIG@")

# The libwebm library directories.
SET(LIBWEBM_LIBRARY_DIRS "@LIBWEBM_LIBRARY_DIRS_CONFIG@")

# A list of all libraries for libwebm.  Those listed here should
# automatically pull in their dependencies.
SET(LIBWEBM_LIBRARIES webm)

# The libwebm library targets.
SET(LIBWEBM_LIBRARY_TARGETS_FILE "@LIBWEBM_LIBRARY_TARGETS_FILE@")
include(${LIBWEBM_LIBRARY_TARGETS_FILE})
